<%if(success && success.length){%>
<div
  class="alert alert-success alert-dismissible fade show col-6 offset-3 position-fixed mt-5"
  role="alert"
  style="border-radius: 2rem; top: 30px; z-index: 5"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%> <%if(error && error.length){%>
<div
  class="alert alert-danger alert-dismissible fade show col-6 offset-3 position-fixed mt-5"
  role="alert"
  style="border-radius: 2rem; top: 30px; z-index: 5"
>
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
